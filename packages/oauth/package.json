{
	"name": "@lucia-auth/oauth",
	"version": "0.3.1",
	"description": "OAuth integration for Lucia",
	"main": "index.js",
	"types": "index.d.ts",
	"module": "index.js",
	"type": "module",
	"files": [
		"**/*"
	],
	"scripts": {
		"build": "shx rm -rf ./dist/* && tsc && shx cp ./package.json ./dist && shx cp ./README.md ./dist && shx cp .npmignore dist",
		"package": "pnpm build && cd dist && pnpm install --no-frozen-lockfile && pnpm publish --no-git-checks --access public && cd ../"
	},
	"keywords": [
		"lucia",
		"lucia-auth",
		"oauth",
		"authentication",
		"auth"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/pilcrowOnPaper/lucia-auth",
		"directory": "packages/lucia"
	},
	"author": "pilcrowonpaper",
	"license": "MIT",
	"exports": {
		"./package.json": "./package.json",
		".": "./index.js",
		"./github": "./github.js",
		"./google": "./google.js",
		"./patreon": "./patreon.js",
		"./reddit": "./reddit.js",
		"./twitch": "./twitch.js"
	},
	"typesVersions": {
		"*": {
			"github": [
				"github.d.ts"
			],
			"google": [
				"google.d.ts"
			],
			"patreon": [
				"patreon.d.ts"
			],
			"reddit": [
				"reddit.d.ts"
			],
			"twitch": [
				"twitch.d.ts"
			]
		}
	},
	"devDependencies": {
		"@types/node": "^18.11.17",
		"lucia-auth": "workspace:*"
	},
	"peerDependencies": {
		"lucia-auth": "0.1.x - 0.4.x"
	}
}
