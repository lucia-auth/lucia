        "*": {
          "client": ["dist/client.d.ts"],
          "types": ["dist/types.d.ts"]
        }
      },
    "repository": {
        "type": "git",
        "url": "https://github.com/pilcrowOnPaper/lucia-sveltekit",
        "directory": "packages/lucia-sveltekit"
    },
    "author": "pilcrowonpaper",
    "license": "MIT",
    "devDependencies": {
        "@rollup/plugin-typescript": "^8.2.1",
        "@sveltejs/kit": "^1.0.0-next.259",
        "@types/bcryptjs": "^2.4.2",
        "@types/cookie": "^0.5.1",
        "@types/jsonwebtoken": "^8.5.1",
        "@typescript-eslint/eslint-plugin": "^4.23.0",
        "@typescript-eslint/parser": "^4.23.0",
        "build-esm": "^4.2.2",
        "esbuild": "^0.12.1",
        "eslint": "^7.26.0",
        "eslint-config-prettier": "^8.3.0",
        "prettier": "^2.3.0",
        "rollup": "^2.48.0",
        "rollup-plugin-esbuild": "^4.2.3",
        "rollup-plugin-multi-input": "^1.3.0",
        "svelte": "^3.38.2",
        "typescript": "^4.7.4",
        "vite": "^3.0.0"
    },
    "dependencies": {
        "@sveltejs/kit": "^1.0.0-next.375",
        "bcryptjs": "^2.4.3",
        "cookie": "^0.5.0",
        "jsonwebtoken": "^8.5.1",
        "nanoid": "^4.0.0"
    }
