{
	"name": "@lucia-auth/sveltekit",
	"version": "0.6.5",
	"description": "SvelteKit integration for Lucia",
	"main": "index.js",
	"types": "index.d.ts",
	"module": "index.js",
	"type": "module",
	"files": [
		"**/*"
	],
	"scripts": {
		"build": "shx rm -rf ./dist/* && tsc && shx cp ./package.json ./dist && shx cp ./README.md ./dist && shx cp .npmignore dist",
		"auri.publish": "pnpm build && cd dist && pnpm install --no-frozen-lockfile && pnpm publish --no-git-checks --access public && cd ../"
	},
	"keywords": [
		"lucia",
		"lucia-auth",
		"sveltekit",
		"svelte",
		"svelte-kit",
		"authentication",
		"auth"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/pilcrowOnPaper/lucia-auth",
		"directory": "packages/lucia"
	},
	"author": "pilcrowonpaper",
	"license": "MIT",
	"exports": {
		"./package.json": "./package.json",
		".": "./index.js",
		"./load": "./load/index.js",
		"./client": "./client/index.js"
	},
	"typesVersions": {
		"*": {
			"client": [
				"client/index.d.ts"
			],
			"load": [
				"load/index.d.ts"
			]
		}
	},
	"devDependencies": {
		"@types/node": "^18.6.2",
		"lucia-auth": "workspace:*",
		"svelte": "3.x"
	},
	"peerDependencies": {
		"lucia-auth": "0.5.x - 0.7.x",
		"svelte": "3.x"
	},
	"dependencies": {
		"@noble/hashes": "^1.1.3"
	}
}
