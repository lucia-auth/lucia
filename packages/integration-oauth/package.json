{
	"name": "@lucia-auth/oauth",
	"version": "1.1.1",
	"description": "OAuth integration for Lucia",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"module": "dist/index.js",
	"type": "module",
	"files": [
		"/dist/",
		"CHANGELOG.md"
	],
	"scripts": {
		"build": "shx rm -rf ./dist/* && tsc",
		"auri.publish": "pnpm build && pnpm publish --no-git-checks --access public"
	},
	"keywords": [
		"lucia",
		"lucia",
		"authentication",
		"auth",
		"oauth"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/pilcrowOnPaper/lucia",
		"directory": "packages/integration-oauth"
	},
	"author": "pilcrowonpaper",
	"license": "MIT",
	"exports": {
		"./package.json": "./dist/package.json",
		".": "./dist/index.js",
		"./providers": "./dist/providers/index.js"
	},
	"typesVersions": {
		"*": {
			"providers": [
				"dist/providers/index.d.ts"
			]
		}
	},
	"devDependencies": {
		"lucia": "latest"
	},
	"peerDependencies": {
		"lucia": "1.x"
	}
}
