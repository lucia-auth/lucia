{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@layouts/*": ["src/layouts/*"],
			"@lib/*": ["src/lib/*"],
			"@components/*": ["src/components/*"],
			"@cela/*": ["integrations/cela/*"],
			"@search/*": ["search/*"],
			"@markdown/*": ["integrations/markdown/*"]
		},
		"jsx": "preserve",
		"jsxImportSource": "solid-js"
	}
}
